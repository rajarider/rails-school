<% if can? :manage, ''  %>
    <div class="rankWrap">
        <h1>Students Rank List</h1>
       
        <table id="rankTable" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Student Name</th>
                    <th>Total Marks</th>
                    <th>Rank</th>
                </tr>
            </thead>
            <% @students_rank.each do |student| %>
                <tbody>
                    <tr>
                        <td><%= student['student_name'] %></td>
                        <td><%= student['student_total'] %></td>
                        <td><%= student['student_rank'] %></td>
                    </tr>
                </tbody>
            <% end %>
        </table>
    </div>
<% else %>
  <h1>Welcome Teacher</h1>
<% end %>
<script>
$(document).ready(function() {
    $('#rankTable').DataTable();
} );
</script>

